{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ecs:*",
                "ec2:*",
                "elasticloadbalancing:*",
                "ecr:*",
                "cloudtrail:LookupEvents",
                "cloudwatch:*",
                "s3:*",
                "ses:*",
                "rds:*",
                "logs:*"
            ],
            "Resource": "arn:aws:logs:eu-west-2:*:*"
        }
    ]
}
